extend ../../../layouts/application

append variables
  - locals.currentPage = "datasets/gem_j_wga";
  - locals.language = "en";
  - locals.title = "Datasets > GEM-J WGA | TogoVar";

block head
  each css in htmlWebpackPlugin.files.css
    link(href=css, rel="stylesheet")

block content
  main
    case TOGOVAR_FRONTEND_REFERENCE
      when 'GRCh37'
        include gem_j_wga_grch37
      when 'GRCh38'
        include gem_j_wga_grch38
      default
        // none

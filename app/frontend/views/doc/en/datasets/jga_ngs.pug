extend ../../../layouts/application

append variables
  - locals.currentPage = "datasets/jga_ngs";
  - locals.language = "en";
  - locals.title = "Datasets > JGA-NGS | TogoVar";

block head
  each css in htmlWebpackPlugin.files.css
    link(href=css, rel="stylesheet")

block content
  main
    case TOGOVAR_FRONTEND_REFERENCE
      when 'GRCh37'
        include jga_ngs_grch37
      when 'GRCh38'
        include jga_ngs_grch38
      default
        // none

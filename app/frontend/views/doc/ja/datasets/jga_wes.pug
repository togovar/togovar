extend ../../../layouts/application

append variables
  - locals.currentPage = "datasets/jga_wes";
  - locals.language = "ja";
  - locals.title = "Datasets > JGA-WES | TogoVar";

block head
  each css in htmlWebpackPlugin.files.css
    link(href=css, rel="stylesheet")

block content
  main
    case TOGOVAR_FRONTEND_REFERENCE
      when 'GRCh37'
        include jga_wes_grch37
      when 'GRCh38'
        include jga_wes_grch38
      default
        // none

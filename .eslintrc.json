{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "jquery" : true,
    "node": true
  },
  "rules": {
    "no-unused-vars": [
      "error", {
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  }
}
